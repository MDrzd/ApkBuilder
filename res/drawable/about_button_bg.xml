<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#ffffffff"
                android:endColor="#ffffffff"
                android:angle="270.0" />
            <stroke
                android:width="1.0dip"
                android:color="#ffffffff" />
            <corners
                android:radius="4.0dip" />
            <padding
                android:left="10.0dip"
                android:top="4.0dip"
                android:right="10.0dip"
                android:bottom="4.0dip" />
        </shape>
    </item>
    <item
        android:state_focused="true">
        <shape>
            <gradient
                android:startColor="#ffffffff"
                android:endColor="#ffffffff"
                android:angle="270.0" />
            <stroke
                android:width="1.0dip"
                android:color="#ffffffff" />
            <corners
                android:radius="4.0dip" />
            <padding
                android:left="10.0dip"
                android:top="4.0dip"
                android:right="10.0dip"
                android:bottom="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ff333333"
                android:endColor="#ff333333"
                android:angle="0.0" />
            <stroke
                android:width="1.0dip"
                android:color="#ffffffff" />
            <corners
                android:radius="4.0dip" />
            <padding
                android:left="10.0dip"
                android:top="4.0dip"
                android:right="10.0dip"
                android:bottom="4.0dip" />
        </shape>
    </item>
</selector>